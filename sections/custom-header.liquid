    <script>
 document.addEventListener("DOMContentLoaded", function () {

        const hideOnScroll = document.getElementById("megaMenu").getAttribute('data-scroll-hide') === "true";
        const isSticky = document.getElementById("megaMenu").getAttribute('data-sticky') === "true";
        var megaMenu = document.getElementById('megaMenu');
        var navigationBar = document.querySelector('.navigation-bar');
        var lastScrollTop = 0;
        const dropdownItems = document.querySelectorAll(".dropdown-item");
       
        //This part shows mega dropdown
        dropdownItems.forEach((item) => {
            item.addEventListener("mouseover", function () {
                showDropdown(item);
            });

            item.addEventListener("mouseout", function () {
                hideDropdown(item);
            });
        });

        function showDropdown(item) {
            const currentDropdown = item.querySelector(".mega-dropdown");
            currentDropdown.style.display = "block";
            currentDropdown.dataset.active = "true"; 
        }

        function hideDropdown(item) {
            const currentDropdown = item.querySelector(".mega-dropdown");
            currentDropdown.style.display = "none";
            currentDropdown.dataset.active = "false"; 
        }
        //this part handles scroll animation and fixed menu
        window.addEventListener('scroll', function () {
            var scrollTop = window.pageYOffset || document.documentElement.scrollTop;

            if (scrollTop > 50 && isSticky) {
            megaMenu.classList.add('fixed-menu');
            navigationBar.style.opacity = '0';

            // Check the scroll direction
            if (scrollTop > lastScrollTop) {
                // Scrolling down
                megaMenu.classList.add('hidden-menu');
            } else {
                // Scrolling up
                if(hideOnScroll) {
                    megaMenu.classList.remove('hidden-menu');
                }
            }

            lastScrollTop = scrollTop;
            } else {
            megaMenu.classList.remove('fixed-menu', 'hidden-menu');
            navigationBar.style.opacity = '1';
            }
        });
    });
           </script>

